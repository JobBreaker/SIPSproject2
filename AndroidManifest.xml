<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sipsproject2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.sipsproject2.SIPSApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name="com.example.sipsproject2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.scanner.ZBarScannerActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.example.sipsproject2.ParkingActivity"
            android:label="@string/title_activity_parking" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.MemberActivity"
            android:label="@string/title_activity_member"
            android:parentActivityName="com.example.sipsproject2.HomeActivity" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.PrivillegeActivity"
            android:label="@string/title_activity_privillege" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.PromotionActivity"
            android:label="@string/title_activity_promotion" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.TrackingActivity"
            android:label="@string/title_activity_tracking" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.HomeActivity"
            android:label="@string/title_activity_home"
            android:parentActivityName="com.example.sipsproject2.MainActivity" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.ActivityGroup"
            android:label="@string/title_activity_activity_group" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.HistoryActivity"
            android:label="@string/title_activity_history" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.ParkingDetailActivity"
            android:label="@string/title_activity_parking_detail" >
        </activity>
        <activity
            android:name="com.example.sipsproject2.ParkingMapFloorActivity"
            android:label="@string/title_activity_parking_map_floor" >
        </activity>
    </application>

</manifest>
