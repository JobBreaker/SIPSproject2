<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sipsproject2"
    android:versionCode="1"
    android:versionName="1.0" >
	<meta-data
	    android:name="com.google.android.maps.v2.API_KEY"
	    android:value="AIzaSyCTCkZuP4zkUJZ9DpS63XoTenZ2IblhHFw">
	</meta-data>
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    

    <application
        android:name="com.example.sipsproject2.SIPSApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"

        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name="com.example.sipsproject2.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.scanner.ZBarScannerActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.example.sipsproject2.ParkingActivity"
            android:label="@string/title_activity_parking" 
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing"
       
            >
        </activity>
        <activity
            android:name="com.example.sipsproject2.MemberActivity"
            android:label="@string/title_activity_member"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan|adjustResize"
