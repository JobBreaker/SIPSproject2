        <activity
            android:name="com.example.sipsproject2.TrackingActivity"
            android:label="@string/title_activity_tracking"
            android:screenOrientation="portrait" >
        </activity>
        
        <activity
            android:name="com.example.sipsproject2.HomeActivity"
            android:label="@string/title_activity_home"
            
